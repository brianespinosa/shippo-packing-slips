{
  "name": "shippo-packing-slips",
  "version": "1.0.0",
  "description": "Automated packing slip generator using Shippo API",
  "keywords": [
    "shippo",
    "packing-slip",
    "shipping",
    "automation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianespinosa/shippo-packing-slips.git"
  },
  "author": "Brian Espinosa <b@bje.co>",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "check": "biome check --write ./",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "generate": "yarn build && node dist/generate-real-pdfs.js",
    "postinstall": "lefthook install",
    "sort-package": "yarn dlx sort-package-json",
    "test:api": "yarn build && node dist/test-shippo.js",
    "test:pdf": "yarn build && node dist/test-pdf.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "17.2.3",
    "pdfkit": "0.17.2",
    "shippo": "2.17.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.3",
    "@types/node": "25.2.0",
    "@types/pdfkit": "0.17.4",
    "lefthook": "2.1.1",
    "typescript": "5.9.3"
  },
  "packageManager": "yarn@4.12.0"
}
